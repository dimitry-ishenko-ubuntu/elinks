	msgdots1="`echo $msgdots2 | sed 's/[a-z]/./g'`"
	msgdots0="`echo $msgdots1 | sed 's/[A-Z]/./g'`"
	msgdots="`echo $msgdots0 | sed 's/[-_ ()]/./g'`"
