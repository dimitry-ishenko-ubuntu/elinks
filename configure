  done
IFS=$as_save_IFS

  test -z "$ac_cv_path_GUILE_CONFIG" && ac_cv_path_GUILE_CONFIG="no"
  ;;
esac
fi
GUILE_CONFIG=$ac_cv_path_GUILE_CONFIG
